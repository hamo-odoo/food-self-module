<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="report_food_order" model="ir.actions.report">
        <field name="name">Order Report</field>
        <field name="model">food.order</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">food-self-module.food_order_template</field>
